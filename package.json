{
  "name": "regex-visualization-thesis",
  "version": "0.0.5",
  "description": "This package is source package for thesis on regex visualization plugin for vs-code.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install": "npm run install:regexer && npm run install:rv && npm run install:rvex",
    "install:regexer": "cd ./libraries/regexer/ && npm i",
    "install:rv": "cd ./libraries/regex-visualization/ && npm i",
    "install:rvex": "cd ./libraries/regex-visualizer-extension/ && npm i",
    "build": "npm run build:regexer && npm run build:rv && npm run build:rvex",
    "release": "npm run vsce --prefix ./libraries/regex-visualizer-extension/",
    "build:regexer": "npm run build --prefix ./libraries/regexer/",
    "build:rv": "npm run build:webpack --prefix ./libraries/regex-visualization/",
    "build:rvex": "npm run build:webpack --prefix ./libraries/regex-visualizer-extension/"
  },
  "author": "Dominik Kundra, KUN0161",
  "license": "ISC",
  "devDependencies": {
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">= 14.17"
  }
}
