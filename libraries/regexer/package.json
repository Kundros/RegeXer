{
  "name": "regexer",
  "version": "0.0.1",
  "description": "Core engine executing regexes step by step.",
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "npm run build:peggy && npm run build:main",
    "build:peggy": "peggy ./src/models/regex.pegjs --cache --plugin ./node_modules/ts-pegjs/dist/tspegjs --extra-options-file pegconfig.json --format es --output ./src/models/regexParser.ts",
    "build:main": "npm run build:main_esm && npm run build:main_common",
    "build:main_esm": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "build:main_common": "tsc --project tsconfig.cjs.json && tsc-alias -p tsconfig.cjs.json",
    "start": "node --trace-warnings dist/cjs/index.js",
    "dev": "node --trace-warnings dist/cjs/index.js",
    "test": "node --experimental-vm-modules --experimental-specifier-resolution=node node_modules/jest/bin/jest.js --detectOpenHandles",
    "test:parser": "node --experimental-vm-modules --experimental-specifier-resolution=node node_modules/jest/bin/jest.js parser --detectOpenHandles",
    "test:matcher": "node --experimental-vm-modules --experimental-specifier-resolution=node node_modules/jest/bin/jest.js matcher --detectOpenHandles"
  },
  "author": "Dominik Kundra, KUN0161",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.7",
    "jest": "^29.7.0",
    "peggy": "^3.0.2",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.8.8"
  },
  "exports": {
    ".": {
      "browser": {
        "import": "./dist/esm/browser/index.js",
        "require": "./dist/cjs/browser/index.js",
        "types": "./types/browser/index.d.js"
      },
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./types/index.d.js"
    }
  },
  "dependencies": {
    "esm": "^3.2.25",
    "ts-pegjs": "^4.2.1"
  }
}
